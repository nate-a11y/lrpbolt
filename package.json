{
  "name": "ride-claim-portal",
  "version": "3.0.0",
  "engines": {
    "node": ">=22 <23"
  },
  "packageManager": "npm@11.4.2",
  "scripts": {
    "preinstall": "node ./scripts/guard-engines.js",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "ci:install": "npm ci --no-progress --foreground-scripts=false --loglevel=error",
    "check:react": "npm ls react react-dom || true",
    "coverage": "vitest run --coverage",
    "dedupe": "npm dedupe || true",
    "deploy:s3": "bash scripts/deploy-s3.sh",
    "dev": "vite",
    "backfill:live-open": "node functions/scripts/backfillLiveOpen.js",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx}\"",
    "images:build": "node scripts/convert-images.mjs",
    "images:verify": "node scripts/verify-webp.mjs",
    "lint": "eslint --ext .js,.jsx src",
    "lint:fix": "eslint . --fix",
    "postinstall": "patch-package && (npm run verify:happy-dom || yarn verify:happy-dom || true)",
    "prebuild": "npm run images:verify",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "analyze": "vite build --mode production && vite-bundle-visualizer || echo \"(optional) bundle visualizer not installed\"",
    "analyze:unused": "node scripts/unused-report.js",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npm-check-updates -u && npm install",
    "deps:unused": "npx depcheck",
    "deps:refresh": "rm -rf node_modules package-lock.json && npm install",
    "deps:why": "npm ls read-package-json npmlog gauge are-we-there-yet @npmcli/move-file",
    "audit": "audit-ci --production",
    "verify:env": "node ./scripts/verify-env.js",
    "verify:prod": "npm run verify:env && npm run lint && npm run format:check && npm run test && npm run build:prod",
    "verify:all": "npm run verify:prod && npm run audit",
    "env:sync": "node ./scripts/sync-env.js",
    "lock:check": "node ./scripts/ensure-lock-sync.mjs",
    "lock:rebuild": "node ./scripts/rebuild-lock.mjs",
    "fix:lock": "npm run lock:rebuild",
    "lh": "lhci autorun || echo \"(optional) LHCI not installed; skipping\"",
    "typecheck": "echo 'JS-only project: skipping TS typecheck'",
    "verify:happy-dom": "node -e \"try{require.resolve('happy-dom/package.json');const v=require('happy-dom/package.json').version; if(parseInt(v.split('.')[0],10)<20) {process.exit(1)} }catch(e){process.exit(1)}\" || (echo 'happy-dom <20 detected. Check overrides/resolutions.' && exit 1)",
    "precommit": "npm run lint && npm run format",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "@mui/system": "^7.0.0",
    "@mui/x-charts": "^8.11.1",
    "@mui/x-data-grid-pro": "^7.0.0",
    "@mui/x-date-pickers-pro": "^7.0.0",
    "@mui/x-license": "^7.0.0",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "@sentry/browser": "^10.10.0",
    "@zxing/browser": "^0.1.5",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dayjs": "1.11.18",
    "dompurify": "^3.2.6",
    "eruda": "^3.4.3",
    "file-saver": "^2.0.5",
    "firebase": "^12.2.1",
    "firebase-admin": "^13.5.0",
    "firebase-functions": "^6.4.0",
    "framer-motion": "^12.23.12",
    "html-to-image": "^1.11.13",
    "html5-qrcode": "^2.3.8",
    "jszip": "^3.10.1",
    "notistack": "^3.0.2",
    "nprogress": "^0.2.0",
    "papaparse": "^5.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^6.0.0",
    "react-qr-code": "^2.0.18",
    "react-router-dom": "^7.8.2",
    "three": "^0.180.0",
    "uuid": "^12.0.0",
    "yet-another-react-lightbox": "^3.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.27.1",
    "@babel/plugin-transform-numeric-separator": "^7.27.1",
    "@babel/plugin-transform-optional-chaining": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.35.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "audit-ci": "^7.1.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "fast-glob": "^3.3.3",
    "globals": "^16.3.0",
    "globby": "^14.1.0",
    "happy-dom": "20.0.2",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "msw": "^2.11.1",
    "npm-check-updates": "^18.1.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "semver": "^7.6.3",
    "sharp": "^0.34.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@grpc/grpc-js": "^1.10.6",
    "@grpc/proto-loader": "^0.7.13",
    "@npmcli/move-file": "^3.0.0",
    "are-we-there-yet": "^4.0.0",
    "color-convert": "^2.0.1",
    "color-name": "^1.1.4",
    "debug": "4.3.7",
    "error-ex": "1.3.2",
    "gauge": "^5.0.0",
    "gaxios": "^6.7.1",
    "glob": "^11.0.3",
    "happy-dom": "20.0.2",
    "google-auth-library": "^9.14.2",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.5",
    "is-arrayish": "0.3.2",
    "npmlog": "^7.0.1",
    "read-package-json": "^7.0.0",
    "rimraf": "^6.0.1",
    "source-map": "^0.7.6",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.4",
    "teeny-request": "^9.0.0"
  },
  "pnpm": {
    "overrides": {
      "happy-dom": "20.0.2"
    }
  },
  "resolutions": {
    "happy-dom": "20.0.2"
  },
  "type": "module"
}
