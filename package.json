{
  "name": "ride-claim-portal",
  "version": "4.0.0",
  "engines": {
    "node": ">=22 <23"
  },
  "packageManager": "npm@11.6.2",
  "scripts": {
    "preinstall": "node ./scripts/guard-engines.js",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "ci:install": "npm ci --no-progress --foreground-scripts=false --loglevel=error",
    "check:react": "npm ls react react-dom || true",
    "coverage": "vitest run --coverage",
    "dedupe": "npm dedupe || true",
    "deploy:s3": "bash scripts/deploy-s3.sh",
    "dev": "vite",
    "backfill:live-open": "node functions/scripts/backfillLiveOpen.js",
    "migrate:important-info-categories": "node functions/scripts/migrateGeneralToPromotions.js",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx}\"",
    "images:build": "node scripts/convert-images.mjs",
    "images:verify": "node scripts/verify-webp.mjs",
    "lint": "eslint --ext .js,.jsx src",
    "lint:fix": "eslint . --fix",
    "scan:dark-hardcodes": "grep -R \"rgba(255,255,255\" -n src || true && grep -R \"#060606\" -n src || true",
    "colors:fix": "python3 ./scripts/replace_colors.py",
    "colors:scan": "python3 ./scripts/scan_colors.py",
    "postinstall": "(npm run fonts:check || true) && patch-package && (npm run verify:happy-dom || yarn verify:happy-dom || true)",
    "prebuild": "npm run colors:scan && npm run images:verify",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "analyze": "vite build --mode production && vite-bundle-visualizer || echo \"(optional) bundle visualizer not installed\"",
    "analyze:unused": "node scripts/unused-report.js",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npm-check-updates -u && npm install",
    "deps:unused": "npx depcheck",
    "deps:refresh": "rm -rf node_modules package-lock.json && npm install",
    "deps:why": "npm ls read-package-json npmlog gauge are-we-there-yet @npmcli/move-file",
    "audit": "audit-ci --production",
    "verify:env": "node ./scripts/verify-env.js",
    "verify:prod": "npm run verify:env && npm run lint && npm run format:check && npm run test && npm run build:prod",
    "verify:all": "npm run verify:prod && npm run audit",
    "env:sync": "node ./scripts/sync-env.js",
    "lock:check": "node ./scripts/ensure-lock-sync.mjs",
    "lock:rebuild": "node ./scripts/rebuild-lock.mjs",
    "fix:lock": "npm run lock:rebuild",
    "lh": "lhci autorun || echo \"(optional) LHCI not installed; skipping\"",
    "typecheck": "echo 'JS-only project: skipping TS typecheck'",
    "verify:happy-dom": "node -e \"try{require.resolve('happy-dom/package.json');const v=require('happy-dom/package.json').version; if(parseInt(v.split('.')[0],10)<20) {process.exit(1)} }catch(e){process.exit(1)}\" || (echo 'happy-dom <20 detected. Check overrides/resolutions.' && exit 1)",
    "precommit": "npm run lint && npm run format",
    "prepare": "husky",
    "fonts:check": "node ./scripts/check-fonts.mjs"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/system": "^7.3.3",
    "@mui/x-charts": "^8.16.0",
    "@mui/x-data-grid-pro": "^8.16.0",
    "@mui/x-date-pickers-pro": "^8.16.0",
    "@mui/x-license": "^8.16.0",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "@sentry/browser": "^10.22.0",
    "@zxing/browser": "^0.1.5",
    "axios": "^1.13.1",
    "cors": "^2.8.5",
    "dayjs": "1.11.19",
    "dompurify": "^3.3.0",
    "eruda": "^3.4.3",
    "file-saver": "^2.0.5",
    "firebase": "^12.5.0",
    "firebase-admin": "^13.5.0",
    "firebase-functions": "^6.6.0",
    "framer-motion": "^12.23.24",
    "html-to-image": "^1.11.13",
    "html5-qrcode": "^2.3.8",
    "jszip": "^3.10.1",
    "notistack": "^3.0.2",
    "nprogress": "^0.2.0",
    "papaparse": "^5.5.3",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^6.0.0",
    "react-qr-code": "^2.0.18",
    "react-router-dom": "^7.9.5",
    "three": "^0.181.0",
    "uuid": "^13.0.0",
    "yet-another-react-lightbox": "^3.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.27.1",
    "@babel/plugin-transform-numeric-separator": "^7.27.1",
    "@babel/plugin-transform-optional-chaining": "^7.28.5",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@eslint/js": "^9.39.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.6",
    "audit-ci": "^7.1.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "fast-glob": "^3.3.3",
    "globals": "^16.4.0",
    "globby": "^15.0.0",
    "happy-dom": "20.0.10",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "msw": "^2.11.6",
    "npm-check-updates": "^19.1.2",
    "patch-package": "^8.0.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "semver": "^7.7.3",
    "sharp": "^0.34.4",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vitest": "^4.0.6"
  },
  "overrides": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-is": "^19.2.0",
    "@grpc/grpc-js": "^1.10.6",
    "@grpc/proto-loader": "^0.7.13",
    "@npmcli/move-file": "^3.0.0",
    "are-we-there-yet": "^4.0.0",
    "color-convert": "^2.0.1",
    "color-name": "^1.1.4",
    "debug": "4.3.7",
    "error-ex": "1.3.2",
    "gauge": "^5.0.0",
    "gaxios": "^6.7.1",
    "glob": "^11.0.3",
    "happy-dom": "20.0.10",
    "google-auth-library": "^9.14.2",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.5",
    "is-arrayish": "0.3.2",
    "npmlog": "^7.0.1",
    "read-package-json": "^7.0.0",
    "rimraf": "^6.1.0",
    "source-map": "^0.7.6",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.4",
    "teeny-request": "^9.0.0"
  },
  "pnpm": {
    "overrides": {
      "happy-dom": "20.0.10"
    }
  },
  "resolutions": {
    "happy-dom": "20.0.10"
  },
  "type": "module"
}
