{
  "name": "ride-claim-portal",
  "version": "3.0.0",
  "engines": {
    "node": "20",
    "npm": ">=10"
  },
  "scripts": {
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "check:react": "npm ls react react-dom || true",
    "coverage": "vitest run --coverage",
    "dedupe": "npm dedupe || true",
    "deploy:s3": "bash scripts/deploy-s3.sh",
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx}\"",
    "images:build": "node scripts/convert-images.mjs",
    "images:verify": "node scripts/verify-webp.mjs",
    "lint": "eslint --ext .js,.jsx src",
    "lint:fix": "eslint . --fix",
    "postinstall": "patch-package",
    "prebuild": "npm run images:verify",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "analyze": "vite build --mode production && vite-bundle-visualizer || echo \"(optional) bundle visualizer not installed\"",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u && npm install",
    "deps:unused": "npx depcheck",
    "deps:refresh": "rm -rf node_modules package-lock.json && npm install",
    "deps:why": "npm ls read-package-json npmlog gauge are-we-there-yet @npmcli/move-file",
    "audit": "npm audit --omit=dev",
    "verify:env": "node ./scripts/verify-env.js",
    "verify:prod": "npm run verify:env && npm run lint && npm run format:check && npm run test && npm run build:prod",
    "verify:all": "npm run verify:prod && npm run audit",
    "env:sync": "node ./scripts/sync-env.js",
    "lh": "lhci autorun || echo \"(optional) LHCI not installed; skipping\"",
    "typecheck": "echo 'JS-only project: skipping TS typecheck'",
    "precommit": "npm run lint && npm run format",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.0",
    "@mui/material": "^7.3.0",
    "@mui/system": "^7.3.0",
    "@mui/x-charts": "^8.9.0",
    "@mui/x-data-grid-pro": "^8.9.0",
    "@mui/x-date-pickers": "^8.9.0",
    "@mui/x-date-pickers-pro": "^8.9.0",
    "@mui/x-license": "^7",
    "@sentry/browser": "^10.7.0",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dayjs": "1.11.13",
    "dompurify": "^3.2.6",
    "firebase": "^12.0.0",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.4.0",
    "framer-motion": "^12.23.6",
    "html-to-image": "^1.11.13",
    "html5-qrcode": "^2.3.8",
    "nprogress": "^0.2.0",
    "papaparse": "^5.5.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^6.0.0",
    "react-qr-code": "^2.0.18",
    "react-router-dom": "^7.6.3",
    "uuid": "^11.1.0",
    "yet-another-react-lightbox": "^3.23.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.24.7",
    "@babel/plugin-transform-numeric-separator": "^7.24.7",
    "@babel/plugin-transform-optional-chaining": "^7.24.8",
    "@babel/plugin-transform-private-methods": "^7.24.7",
    "@babel/plugin-transform-private-property-in-object": "^7.24.7",
    "@babel/preset-env": "^7.25.0",
    "@eslint/js": "^9.33.0",
    "@sentry/vite-plugin": "^2.18.0",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.1.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "depcheck": "^1.4.3",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "globby": "^14.0.1",
    "husky": "^9.0.0",
    "jsdom": "^25.0.0",
    "msw": "^2.10.4",
    "npm-check-updates": "^16.14.12",
    "patch-package": "^8.0.0",
    "prettier": "^3.3.3",
    "sharp": "^0.34.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.1",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4",
    "workbox-build": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "overrides": {
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.4",
    "source-map": "^0.7.6",
    "glob": "^11.0.3",
    "rimraf": "^6.0.1",
    "read-package-json": "^7.0.0",
    "npmlog": "^7.0.1",
    "gauge": "^5.0.0",
    "are-we-there-yet": "^4.0.0",
    "@npmcli/move-file": "^3.0.0"
  },
  "type": "module"
}
