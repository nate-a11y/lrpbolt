{
  "name": "ride-claim-portal",
  "version": "3.0.0",
  "engines": {
    "node": "20",
    "npm": ">=10"
  },
  "scripts": {
    "build": "vite build",
    "check:react": "npm ls react react-dom || true",
    "coverage": "vitest run --coverage",
    "dedupe": "npm dedupe || true",
    "deploy:s3": "bash scripts/deploy-s3.sh",
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "images:build": "node scripts/convert-images.mjs",
    "images:verify": "node scripts/verify-webp.mjs",
    "lint": "eslint --ext .js,.jsx src",
    "lint:fix": "eslint . --fix",
    "postinstall": "patch-package",
    "prebuild": "npm run images:verify",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.0",
    "@mui/material": "^7.3.0",
    "@mui/system": "^7.3.0",
    "@mui/x-data-grid-pro": "^8.9.0",
    "@mui/x-date-pickers": "^8.9.0",
    "@mui/x-date-pickers-pro": "^8.9.0",
    "@mui/x-charts": "^8.9.0",
    "@mui/x-license": "^7",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.6",
    "firebase": "^12.0.0",
